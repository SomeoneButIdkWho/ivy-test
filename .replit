run = "python keep_alive.py & watchmedo auto-restart --pattern='*.py' --recursive python bot.py"
language = "python3"
entrypoint = "bot.py"
modules = ["python-3.11"]

[[ports]]
localPort = 5000
externalPort = 80

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Discord Bot"

[[workflows.workflow]]
name = "Discord Bot"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python bot.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"
